  <DataSets>
    <DataSet Name="DataSet1">
      <Query>
        <DataSourceName>DataSource1</DataSourceName>
        <CommandText>SELECT s.USER_ID, s.USERNAME, COUNT(b.USER_ID) AS Total_Borrowed_Books
FROM STUDENT s
LEFT JOIN BORROW b ON s.USER_ID = b.USER_ID
GROUP BY s.USER_ID, s.USERNAME;</CommandText>
